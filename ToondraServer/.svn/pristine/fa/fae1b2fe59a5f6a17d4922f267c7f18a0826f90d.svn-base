package kr.co.toondra.api.community.service;

import kr.co.toondra.api.bean.Result;
import kr.co.toondra.api.community.dao.FAQDao;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service("apiFAQService")
public class FAQService {

	@Autowired
	private FAQDao dao;

	public Result getFAQList() {

		Result result = new Result();
		result.putObject("total_cnt", dao.getFAQListCount());
		result.putObject("notice_list", dao.getFAQList());
		
		return result;
	}

}
