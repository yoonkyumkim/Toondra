.navbar-brand-ahope:focus, .navbar-brand-ahope:hover {
	color: #F37023;
	background-color: transparent;
	text-decoration: none;
	font-size: 23px;
}

.navbar-brand-ahope {
	font-weight: bold;
	color: #F37023;
	float: left;
	height: 50px;
	padding: 15px 15px;
	font-size: 20px;
	line-height: 20px;
}

.navbar-top-ahope {
	background-color: #000000 !important;
}
.navbar-top-ahope:hover, .navbar-top-ahope:focus  {
	background-color: #000000 !important;
	color: #70BDFF;
}
.sidebar {
	background-color: #f8f8f8;
}