/* Generic */
.vertical-center {
	vertical-align: middle;
}

.ext-span {
	font-weight:bold; 
	color: #333333;
	font-size:11px;
	}

/* Complex table style */
.table {
	width: 100%;
	border: 1px solid #cccccc;
	}
	.table-header + .table {
		border-top: 0;
	}

	.table > thead,
	.table > tbody > tr > th,
	.table > tfoot > tr > th {
		text-align: left;
	}
	.table > thead > tr > th,
	.table > thead > tr > td,
	.table > tfoot > tr > th,
	.table > tfoot > tr > td {
		padding: 9px 10px;
		border: 1px solid #cccccc;
		-webkit-box-shadow: inset 1px 0 0 rgba(255, 255, 255, 0.85), inset 0 1px 0 rgba(255, 255, 255, 0.85);
		-moz-box-shadow: inset 1px 0 0 rgba(255, 255, 255, 0.85), inset 0 1px 0 rgba(255, 255, 255, 0.85);
		box-shadow: inset 1px 0 0 rgba(255, 255, 255, 0.85), inset 0 1px 0 rgba(255, 255, 255, 0.85);
		}
		.table-header + .table > thead > tr > th,
		.table-header + .table > thead > tr > td {
			border-top: 0;
		}

	.table > tbody > tr > th,
	.table > tbody > tr > td {
		padding: 9px 10px;
		border-top: 1px solid #e6e6e6;
		border-left: 1px dotted #cfcfcf;
		}

		/* Even rows styling */
		.table > tbody > tr:nth-child(even) {
			background: #f7f7f7;
		}
		.table > tbody > tr.even {
			background: #f7f7f7;
		}

		/* Hover styling */
		.table > tbody > tr:hover {
			background: #f0f0f0;
		}

/* Special cell effect */
.row-drop > th,
.row-drop > td,
.cell-drop {
	padding: 20px 20px;
	position: relative;
	}
	.table > tbody > tr.row-drop > th,
	.table > tbody > tr.row-drop > td,
	.table > tbody > tr > .cell-drop {
		border-top: 0;
	}
.row-drop_box {color:#666666;}
.row-drop_box .q_area {background:url(../../img/faq_icon_q.png) no-repeat; color:#222222; font-weight:bold; margin:10px 10px; padding:2px 0 0 28px;}
.row-drop_box .a_area {background:url(../../img/faq_icon_a.png) no-repeat; color:#666666; font-weight:bold; margin:10px 10px; padding:2px 0 0 28px;}

/* Padding adjust */
th.low-padding,
td.low-padding {
	line-height: 24px !important;
	padding: 5px 5px 4px 5px !important;
}
td.button-height,
th.button-height {
	padding: 8px 10px 8px 10px !important;
}

/* Header and footer for the complex table style */
.table-header,
.table-footer {
	padding: 10px;
	border-width: 1px;
	border-style: solid;
	}
	.table-header {
		-webkit-border-top-left-radius: 10px;
		-webkit-border-top-right-radius: 10px;
		-moz-border-radius: 10px 10px 0 0;
		border-radius: 10px 10px 0 0;
		-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
		-moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
		box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
	}
	.table-footer {
		-webkit-border-bottom-left-radius: 10px;
		-webkit-border-bottom-right-radius: 10px;
		-moz-border-radius: 0 0 10px 10px;
		border-radius: 0 0 10px 10px;
	}
	.table-header.button-height,
	.table-footer.button-height {
		margin: 0;
		padding: 9px 10px;
	}
	.table-header.silver-gradient {
		-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.75);
		-moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.75);
		box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.75);
	}
	.table-header.black-gradient,
	.table-header.anthracite-gradient,
	.table-header.grey-gradient {
		-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
		-moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
		box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);
	}
	.table-header.red-gradient,
	.table-header.orange-gradient,
	.table-header.green-gradient {
		-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
		-moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
		box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.35);
	}

/* Simple table style */
.simple-table {
	width: 100%;
	background-color: #ffffff;
	}
	.simple-table > thead > tr > td {border-top:1px solid #e6e6e6;color: black;padding: 8px 12px;}
	.simple-table > thead > tr > th,
	.simple-table > tfoot > tr > th,
	.simple-table > tfoot > tr > td {
		border-top:2px solid #000;
		color: black;
		padding: 8px 12px;
		}
		.simple-table > thead > tr > th,
		.simple-table > thead > tr > td {
			font-weight: bold;
			font-size: 11px;
			color:#666666;
			border-right: 1px solid #c7c7c7;
		}
		.simple-table > thead > tr > th.hls-k,
		.simple-table > thead > tr > td.hls-k,
		.simple-table > tbody > tr > th.hls-k,
		.simple-table > tbody > tr > td.hls-k,
		.simple-table > tfoot > tr > th.hls-k,
		.simple-table > tfoot > tr > td.hls-k  {
			border-right: 0px;
		}
		.simple-table > tfoot > tr > th,
		.simple-table > tfoot > tr > td {
			border-top: 1px solid #cccccc;
			color: #666;
			font-size:11px;
			font-weight:bold;
			padding: 8px 12px;
			background: #f7f7f7;
			border-right: 1px dotted #c7c7c7;
		}
		.simple-table > tfoot > tr > th.bt-k,
		.simple-table > tfoot > tr > td.bt-k {
			padding: 8px 12px;
			border-right: 1px dotted #c7c7c7;
			border-bottom:2px solid #000;
		}
	.simple-table > tbody > tr > th,
	.simple-table > tbody > tr > td {
		padding: 8px 12px;
		color: #333333;
		border-top: 1px solid #e6e6e6;
		border-right: 1px dotted #908d8d;
		font-size:11px;
		}
		.simple-table > tbody > tr:first-child > th,
		.simple-table > tbody > tr:first-child > td {
			border-top-color: #e0e2e4;
		}
		.simple-table > thead,
		.simple-table > tbody > tr > th,
		.simple-table > tfoot > tr > th {
			text-align: left;
		}
		/* Even rows styling */
		.simple-table > tbody > tr:nth-child(even) {
			background: #f7f7f7;
		}
		.simple-table > tbody > tr.even {
			background: #f7f7f7;
		}

		/* Hover styling */
		.simple-table > tbody > tr:hover {
			background: #f0f0f0;
		}
		
