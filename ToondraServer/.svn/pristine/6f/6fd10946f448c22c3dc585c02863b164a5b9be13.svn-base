.only-under {border-bottom:1px solid #e6e6e6 !important;}
.no-under {border:none !important;}
.fake_thda {border-top:2px solid #000 !important;}
/* Utility classes */
.checkbox-cell {
	width: 13px;
}


/* Table sorter classes */
.header:hover {
	color: #0059a0;
	cursor: pointer;
}
.headerSortUp:before,
.headerSortDown:before {
	font-size: 0.75em;
	padding-right: 2px;
	vertical-align: 0.15em;
	}
	.headerSortUp:before {
		content: '▲';
	}
	.headerSortDown:before {
		content: '▼';
	}

/* Classes for responsive tables */
.responsive-table-on .hide-on-mobile-portrait,
.responsive-table-on .hide-on-mobile-landscape,
.responsive-table-on .hide-on-mobile,
.responsive-table-on .hide-on-tablet-portrait,
.responsive-table-on .hide-on-tablet-landscape,
.responsive-table-on .hide-on-tablet {
	display: none !important;
}
.responsive-table-on .forced-display {
	display: table-cell !important;
}

/* UI Object */

.gap_r1{margin-right:10px;}
.gap_r2{margin-right:5px;}
.gap_r3{margin-right:20px;}
.gap_t1{margin-top:1px;}
.gap_t2{margin-top:5px;}
.board_style_des{clear:both; height:20px;}
.board_style_des .fir{list-style:none; float:left; margin-left:-17px; margin-bottom:20px;}
.board_style_des .sec{list-style:none; float:right; margin-right:8px; margin-bottom:20px;}
/* //UI Object */
.button_center{text-align:center;}
th.ver-middle,
td.ver-middle {
vertical-align: middle;}

/*480px __________________________________________________________________________________________________________ */
@media only screen and (min-width: 480px) {

	/* Classes for responsive tables */
	.responsive-table-on .hide-on-mobile-portrait {
		display: table-cell !important;
	}
	.button_right{float:none; text-align:center; margin:10px auto !important;}
	
}/*/mediaquery*/

/*768px __________________________________________________________________________________________________________ */
@media only screen and (min-width: 768px) {

	/* Classes for responsive tables */
	.responsive-table-on .hide-on-mobile-landscape,
	.responsive-table-on .hide-on-mobile {
		display: table-cell !important;
	}
	.button_right{float:none; text-align:center; margin:10px auto !important;}
	

}/*/mediaquery*/

/*992px __________________________________________________________________________________________________________ */
@media only screen and (min-width: 992px) {

	/* Classes for responsive tables */
	.responsive-table-on .hide-on-tablet-portrait {
		display: table-cell !important;
	}
	.button_right{float:right; margin-top:-44px !important;}

}/*/mediaquery*/

/*1200px __________________________________________________________________________________________________________ */
@media only screen and (min-width: 1200px) {

	/* Classes for responsive tables */
	.responsive-table-on .hide-on-tablet-landscape,
	.responsive-table-on .hide-on-tablet {
		display: table-cell !important;
	}
	.button_right{float:right; margin-top:-44px !important;}

}/*/mediaquery*/